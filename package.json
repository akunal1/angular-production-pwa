{
  "name": "angular-boilerplate",
  "version": "1.0.1",
  "description": "Angular starter for large front-end projects built under a clean architecture that helps to scale and maintain a fast workflow.",
  "author": "<My Name>",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:es": "ng serve -c=es --port 4201",
    "start:fr": "ng serve -c=fr --port 4202",
    "start:ja": "ng serve -c=ja --port 4203",
    "build": "ng build --prod",
    "build:prod": "ng build --prod",
    "build:optimized": "ng build --prod --build-optimizer --aot",
    "build:stats": "ng build --stats-json",
    "build:i18n": "npm run build -- --localize",
    "build:i18n:prod": "npm run build:prod -- --localize",
    "build:i18n:optimized": "npm run build:optimized -- --localize",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract": "ng xi18n --output-path src/locale --ivy",
    "analyze": "webpack-bundle-analyzer dist/angular-boilerplate/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.0",
    "@angular/common": "~11.0.0",
    "@angular/compiler": "~11.0.0",
    "@angular/core": "~11.0.0",
    "@angular/forms": "~11.0.0",
    "@angular/platform-browser": "~11.0.0",
    "@angular/platform-browser-dynamic": "~11.0.0",
    "@angular/router": "~11.0.0",
    "@angular/service-worker": "~11.0.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "~11.0.0",
    "@angular/localize": "^11.0.0",
    "@fullhuman/postcss-purgecss": "^3.1.3",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.16",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^4.1.0",
    "postcss-scss": "^3.0.4",
    "protractor": "~7.0.0",
    "tailwindcss": "^1.9.6",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.0.5",
    "webpack-bundle-analyzer": "^4.3.0"
  }
}
